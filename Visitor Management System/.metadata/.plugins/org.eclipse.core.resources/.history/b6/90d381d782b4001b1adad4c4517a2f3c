<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.manage.system.mapper.SigninMapper">
	<select id="selectBoardDetail" parameterType="UserDTO"
		resultType="UserDTO">
		SELECT seq, userId, userPw, userPwEnc, name, tel, phone, email, zipCode, addr1, addr2, remoteIp, level
        FROM adm_member
        WHERE userId=#{userId}
        AND userPw=#{userPw}
	</select>
</mapper>

